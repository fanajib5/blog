{
  "name": "blogfolio-najib",
  "version": "1.0.0",
  "type": "module",
  "description": "Situs pribadi Faiq Najib - built with Eleventy",
  "main": "index.js",
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev:11ty": "eleventy --serve --watch",
    "dev:sass": "sass --watch src/assets/scss/style.scss:_site/assets/css/style.css",
    "build": "npx @11ty/eleventy",
    "build:11ty": "eleventy",
    "build:sass": "sass src/assets/scss/style.scss:_site/assets/css/style.css --style=compressed",
    "clean": "rimraf _site"
  },
  "keywords": [
    "blog",
    "eleventy",
    "static-site"
  ],
  "author": "Faiq Najib",
  "license": "CC-BY-NC-4.0",
  "devDependencies": {
    "@11ty/eleventy": "^3.1.2",
    "markdown-it": "^14.1.0",
    "markdown-it-anchor": "^9.2.0",
    "markdown-it-toc-done-right": "^4.2.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.1.2",
    "sass": "^1.94.2"
  },
  "engines": {
    "node": ">=18.17.0"
  }
}
